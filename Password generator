import random
# Ask for user's name
def password_generator():
    name = input("What is your name? ").lower()
    # Create a list of numbers
    numbers = ["123", "556", "789", "5566", "2002" "2025"]
    # Give the numbers a random choice
    number = random.choice(numbers)
    # Create a variable and add name and number
    password = f"Your password is: {name}{number}"
    # Print the password
    print(password)
# Call the function
password_generator()
